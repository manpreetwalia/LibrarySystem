package library.daos;

import library.entities.Book; // Import entities from the project
import library.interfaces.daos.IBookHelper;// import interfaces 
import library.interfaces.entities.IBook; // 

public class BookHelper implements IBookHelper
{

	
	public IBook makeBook(String author, String title, String callNumber, int id)
	{
		return new Book(author, title, callNumber, id);	 //return detail of book 
	}

}
